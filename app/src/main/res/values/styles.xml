<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.ActionBar.Transparent" parent="AppTheme">
        <item name="colorPrimaryDark">@android:color/transparent</item>
    </style>

    <style name="AppTheme.ActionBar.Visible" parent="AppTheme">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">false</item>
    </style>



    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ClassicBtn" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/normal_btn</item>
    </style>
    <style name="Btn"></style>
    <style name="Btn_transparent" parent="Btn">
        <item name="android:background">#00000000</item>
    </style>
    <style name="btn_back" parent="Btn_transparent">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:src">@drawable/ic_back</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
