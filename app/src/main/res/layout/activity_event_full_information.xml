<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.denero.handmadeevent.EventList.EventFullInformationActivity">

    <Button
        android:id="@+id/event_full_info_subscribe_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="subscribe"/>

    <Button
        android:id="@+id/event_full_info_unsub_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="unsub"/>

    <TextView
        android:id="@+id/event_full_info_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
